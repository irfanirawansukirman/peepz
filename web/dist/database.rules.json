{
  "rules": {
    ".read": "auth.token.email.endsWith('novoda.com')",
    ".write": "auth.token.email.endsWith('novoda.com')",

    "wip": {
      "rooms": {
        "$room": {
          ".read": "root.child('user').child(auth.uid).child($room).exists()",
          ".write": "false"
        }
      }
    }

  }
}
