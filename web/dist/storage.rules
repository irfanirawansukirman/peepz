service firebase.storage {
  match /b/peepz-2f400.appspot.com/o {
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }

    match /wip/rooms/{roomId}/wall/{userId}/* {
      allow read: if request.auth != null;
      allow write:
        if request.auth.uid == userId;
    }

  }
}
